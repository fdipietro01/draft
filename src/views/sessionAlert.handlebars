{{#if success}}
<div id="alert-success" class="alert alert-info text-center mx-auto" style="width: 400px; margin-top:100px;" role="alert">
    <span> {{message}}</span>
</div>
<div class="text-center">
    <a class="btn btn-info px-3" href={{url}}>Ir al {{case}}</a>
</div>
{{else}}

<div id="alert-error" class="alert alert-danger text-center mx-auto" style="width: 400px; margin-top:100px;" role="alert">
    <span> {{message}}</span>
</div>
<div class="text-center">
    <a class="btn btn-danger px-3" href={{url}}> Volver al {{case}}</a>
</div>
{{/if}}

